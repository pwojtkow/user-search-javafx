<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>


<BorderPane prefHeight="700.0" prefWidth="800.0" maxHeight="Infinity" maxWidth="Infinity" xmlns="http://javafx.com/javafx/8.0.66" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.starterkit.javafx.controller.UserProfileController">
   <left>
      <Separator maxWidth="45.0" orientation="VERTICAL" prefHeight="297.0" prefWidth="35.0" BorderPane.alignment="CENTER" />
   </left>
   <right>
      <Separator maxWidth="45.0" orientation="VERTICAL" prefHeight="330.0" prefWidth="35.0" BorderPane.alignment="CENTER" />
   </right>
   <bottom>
      <Separator maxHeight="45.0" prefHeight="35.0" prefWidth="600.0" BorderPane.alignment="CENTER" />
   </bottom>
   <center>
      <GridPane BorderPane.alignment="CENTER" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="5.0" prefWidth="5.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="%userLogin.label.text" />
            <Label text="%firstName.label.text" GridPane.rowIndex="1" />
            <Label text="%lastName.label.text" GridPane.rowIndex="2" />
            <Label text="%email.label.text" GridPane.rowIndex="3" />
            <Label text="%password.label.text" GridPane.rowIndex="4" />
            <Label text="%aboutMe.label.text" GridPane.rowIndex="5" />
            <TextField fx:id="login" disable="true" GridPane.columnIndex="1" />
            <TextField fx:id="name" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="surname" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField fx:id="email" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <TextField fx:id="password" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <TextArea fx:id="aboutMe" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="5" />
            <TextArea fx:id="lifeMotto" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="4" GridPane.rowIndex="5" />
            <Label text="%lifeMotto.label.text" GridPane.columnIndex="3" GridPane.rowIndex="5" />
            <HBox spacing="30" GridPane.columnIndex="4" GridPane.rowIndex="7">
            <Button fx:id="cancelButton" onAction="#cancelButtonAction" mnemonicParsing="false" text="%cancelButton.text" />
            <Button fx:id="saveButton" onAction="#saveButtonAction" mnemonicParsing="false" text="%saveButton.text" />
            </HBox>
         </children>
      </GridPane>
   </center>
</BorderPane>
